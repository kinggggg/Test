<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context.xsd
                           http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop.xsd">

    <aop:aspectj-autoproxy/>

    <bean id="user" class="com.zeek.domain.User">
        <property name="id"  value="1"></property>
        <property name="name" value="zhangsan"></property>
        <property name="age" value="20"></property>
    </bean>

    <bean class="com.zeek.domain.concert.Audience" />

    <bean id="performance" class="com.zeek.domain.concert.Performance" />

    <bean id="performance2" class="com.zeek.domain.concert.Performance2" />

</beans>